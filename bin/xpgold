#!/usr/bin/ruby
require 'yaml'
$: << File.join(File.dirname(__FILE__), "..", "lib")

require 'provision'
require 'pp'

result = Provision::Factory.new.xp_gold_image({
  :ieversion => "8",
  :master_image_url => 'http://localhost/vm-images/dev-seliex-master.img',
  :hostname  => "xpgold",
  :networks  => [:mgmt],
  :qualified_hostnames => {
    :mgmt => "xpgold.mgmt.dev.net.local"
  }
})

pp result


